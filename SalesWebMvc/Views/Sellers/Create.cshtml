@model SalesWebMvc.Models.Seller;

@{
    ViewData["Title"] = "Create";
}

<h2>@ViewData["Title"]</h2>

<h4>Seller</h4>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Name" class="custom-control-label"></label>
                <input asp-for="Name" class="form-control"/>
            </div>
            <div class="form-group">
                <label asp-for="Email" class="custom-control-label"></label>
                <input asp-for="Email" class="form-control" />
            </div><div class="form-group">
                <label asp-for="BirthDate" class="custom-control-label"></label>
                <input asp-for="BirthDate" class="form-control" />
            </div><div class="form-group">
                <label asp-for="BaseSalary" class="custom-control-label"></label>
                <input asp-for="BaseSalary" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn-success"/>
            </div>
        </form>
    </div>
</div>